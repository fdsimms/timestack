{
  "manifest_version": 2,

  "name": "Timestack",
  "description": "",
  "version": "0.0.0.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  }
}
